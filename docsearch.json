{
  "index_name": "doc",
  "start_urls": ["https://ohmymn.marginnote.cn"],
  "selectors": {
    "lvl0": "",
    "lvl1": ".content  h1",
    "lvl2": ".content  h2",
    "lvl3": ".content  h3",
    "lvl4": ".content  h4",
    "lvl5": ".content  h5",
    "content": ".content p, .content li"
  },
  "lang": {
    "selector": "/html/@lang",
    "type": "xpath",
    "global": true
  },
  "strip_chars": " .,;:#",
  "selectors_exclude": [".sr-only"],
  "custom_settings": {
    "attributesForFaceting": ["lang", "tags"]
  }
}
